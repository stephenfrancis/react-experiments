{
  "name": "react-experiments",
  "version": "1.0.0",
  "description": "experimental",
  "main": "dist/index.html",
  "scripts": {
    "build": "npx parcel build src/index.html",
    "clean": "rm -f -r dist/",
    "exec": "node src/Server.js",
    "local": "npx parcel serve src/index.html",
    "test": "npx jest"
  },
  "author": "",
  "repository": {
    "type": "git",
    "url": "git@github.com:stephenfrancis/react-experiments.git"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "node_modules"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@types/koa": "^2.11.4",
    "@types/node": "^14.11",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "axios": "^0.20",
    "http-graceful-shutdown": "^2.3.2",
    "koa": "^2.13.0",
    "koa-static": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^26.0.15",
    "jest": "^26.6.1",
    "jest-dom": "^4.0.0",
    "parcel": "^2.0.0-nightly.425",
    "ts-jest": "^26.4.2"
  }
}
